    "build:react-native": "rimraf build/react-native && babel react-native -d build/react-native"
  },
  "author": "Nguyen Anh Tuan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}
